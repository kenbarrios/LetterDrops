<body class="fallingPages">
<nav>
  <a href="/menu">
    <h1>
      Alphabetized Level
    </h1>
  </a>
  <li class="navLink">
    <%= form_tag('/log_out', method: :delete, :id => "logOut") do %>
     <%= submit_tag "Sign-Out" %>
     <% end %>
  </li>
  <a href="/alpha">
    <li class="navLink">
      Replay
    </li>
  </a>
</nav>

<p class='instructions'>Press the corresponding keys to pop the LetterDrops!</p>

<% ('a'..'z').to_a.each do |letter| %>
  <div class="div<%= letter.upcase %>">
    <%= letter %>
  </div>
<% end %>
</body>
